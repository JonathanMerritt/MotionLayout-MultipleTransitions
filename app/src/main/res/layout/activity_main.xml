<androidx.constraintlayout.motion.widget.MotionLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activityMain_motion"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorSubSurface"
    app:layoutDescription="@xml/activity_main_scene"
    tools:context=".MainActivity">

  <View android:id="@+id/activityMain_toolbar"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:background="@drawable/ll_surface_background" />

  <View android:id="@+id/activityMain_toolbar_handle"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:background="@drawable/ll_handle_background" />

  <View android:id="@+id/activityMain_drawer"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:background="@drawable/ll_surface_background" />

  <View android:id="@+id/activityMain_drawer_handle"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:background="@drawable/ll_handle_background" />

  <View android:id="@+id/activityMain_sheet"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:background="@drawable/ll_surface_background" />

  <View android:id="@+id/activityMain_sheet_handle"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:background="@drawable/ll_handle_background" />
</androidx.constraintlayout.motion.widget.MotionLayout>
